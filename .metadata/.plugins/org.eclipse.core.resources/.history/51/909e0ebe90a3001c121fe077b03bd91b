
public class Ex14_0 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		Object obj -> (a, b) a > b ? a : b; // 람다식, 익명객체
		Object obj = new Object() {
			int max(int a, int b) {
				return a > b ? a : b;
			}
		};

		int value =  obj.max(3, 5); // 함수형 인터페이스 
	}

}
